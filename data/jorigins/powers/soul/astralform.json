{
	"name": "Astral Form",
	"description": "Disconnected from your body, you are forcibly brought back to it if you wander too far or it is damaged. You can return at will using your active ability.",
	"type": "origins:multiple",
	"togglepower": {
		"type": "origins:active_self",
		"entity_action": {
			"type": "origins:execute_command",
			"command": "function jorigins:astralend",
			"permission_level": 4
		},
		"cooldown": 20,
		"hud_render": {
			"should_render": false
		},
		"key": {
	  		"key": "key.origins.primary_active"
  		}
	},
	"revert": {
		"type": "origins:action_on_death",
		"bientity_action": {
			"type": "origins:target_action",
			"action": [
				{
					"type": "origins:execute_command",
					"command": "function jorigins:astralend",
					"permission_level": 4
				}
			]
		}
	},
	"distance_check":{
	"type": "origins:action_over_time",
  	"entity_action": {
    	"type": "origins:execute_command",
    	"command": "function jorigins:soulcheck",
    	"permission_level": 4
  	},
  "interval": 10
	},
	"setup":
	{
		"type": "origins:action_on_callback",
		"entity_action_lost": {
			"type": "origins:and",
    "actions": [
		{
			"type": "origins:execute_command",
			"command": "kill @e[tag =HostBody]"
		},
		{
			"type": "origins:execute_command",
			"command": "kill @e[type=item_display]"
		}
		]
		},
    "entity_action_gained":{
      "type": "origins:and",
    "actions": [
		{
			"type": "origins:add_velocity",
      "y": 1
		}
		]
    },
		"execute_chosen_when_orb": true
	},
  "nightvision": {
      "type": "origins:night_vision",
      "strength": 0.15,
      "condition": {
        "type": "origins:brightness",
        "comparison": "<=",
        "compare_to": 0.408
      }
  },
  "soul_look": {
    "type": "origins:model_color",
    "red": 1,
    "green": 1,
    "blue": 1,
    "alpha": 0.7
},
"overlay_close": {
    "type": "origins:overlay",
    "hidden": true,
    "sprite": "*:soulverlaya",
    "strength": 0.5,
    "draw_mode": "texture",
    "draw_phase": "below_hud",
    "visible_in_third_person": true,
    "condition": {
        "type": "origins:resource",
        "resource": "jorigins:soul/astralform_overlay_resource",
        "compare_to": 0,
        "comparison": "=="
      }
  },
  "overlay_mid": {
    "type": "origins:overlay",
    "hidden": true,
    "sprite": "*:soulverlayb",
    "strength": 0.6,
    "draw_mode": "texture",
    "draw_phase": "below_hud",
    "visible_in_third_person": true,
    "condition": {
        "type": "origins:resource",
        "resource": "jorigins:soul/astralform_overlay_resource",
        "compare_to": 1,
        "comparison": "=="
      }
  },
  "overlay_edge": {
    "type": "origins:overlay",
    "hidden": true,
    "sprite": "*:soulverlayc",
    "strength": 0.7,
    "draw_mode": "texture",
    "draw_phase": "below_hud",
    "visible_in_third_person": true,
    "condition": {
        "type": "origins:resource",
        "resource": "jorigins:soul/astralform_overlay_resource",
        "compare_to": 2,
        "comparison": "=="
      }
  },
  "overlay_resource": {
      "type": "origins:resource",
      "min": 0,
      "max": 2,
      "hud_render":
          {
              "sprite_location": "*:resourcecustom.png",
              "bar_index": 1,
              "should_render": false
          }
    }
}