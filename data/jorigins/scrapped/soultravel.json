{
  "name": "SoulTravel",
  "description": "AnotherTestPower",
  "type": "origins:multiple",
  "poweractive": {
      "type": "origins:resource",
      "min": 0,
      "max": 1,
      "hud_render": {
          "should_render": false
      },
      "start_value": 1,
      "min_action": {
          "type": "origins:execute_command",
          "command": "function jorigins:summon",
          "permission_level": 4
      },
      "max_action": {
          "type": "origins:execute_command",
          "command": "function jorigins:unsummon",
          "permission_level": 4
      }
  },
  "travel": {
      "type": "origins:active_self",
      "entity_action": {
          "type": "origins:if_else_list",
          "actions": [
              {
                  "condition": {
                      "type": "origins:and",
                      "conditions": [
                          {
                              "type": "origins:resource",
                              "resource": "jorigins:soultravel_poweractive",
                              "comparison": "==",
                              "compare_to": 1
                          }
                      ]
                  },
                  "action": {
                      "type": "origins:change_resource",
                      "resource": "jorigins:soultravel_poweractive",
                      "change": 0,
                      "operation": "set"
                  }
              },
              {
                  "condition": {
                      "type": "origins:and",
                      "conditions": [
                          {
                              "type": "origins:resource",
                              "resource": "jorigins:soultravel_poweractive",
                              "comparison": "==",
                              "compare_to": 0
                          }
                      ]
                  },
                  "action": {
                      "type": "origins:change_resource",
                      "resource": "jorigins:soultravel_poweractive",
                      "change": 1,
                      "operation": "set"
                  }
              }
          ]
      },
      "cooldown": 20,
      "hud_render": {
          "should_render": false
      },
      "key": {
    "key": "key.origins.primary_active"
}
  },
  "disable":
  {
      "type": "origins:action_on_callback",
      "entity_action_lost": {
          "type": "origins:execute_command",
          "command": "kill @e[tag=soul]"
      },
      "execute_chosen_when_orb": true
  },
  "flight":
  {
    "type": "origins:creative_flight",
    "condition": 
        {
            "type": "origins:resource",
            "resource": "jorigins:soultravel_poweractive",
            "comparison": "==",
            "compare_to": 0
        }
  },
  "phase":
  {
    "type": "origins:phasing",
    "blacklist": true,
    "render_type": "blindness",
    "view_distance": 10,
    "block_condition": {
        "type": "origins:in_tag",
        "tag": "origins:unphasable"
    },
    "condition": 
        {
            "type": "origins:resource",
            "resource": "jorigins:soultravel_poweractive",
            "comparison": "==",
            "compare_to": 0
        }
  },
  "blocked":
  {
    "type": "origins:prevent_block_place",
    "entity_action": {
        "type": "origins:execute_command",
        "command": "tellraw @s {\"text\": \"Cannot place a block here!\", \"color\": \"red\"}"
    },
    "condition": 
        {
            "type": "origins:resource",
            "resource": "jorigins:soultravel_poweractive",
            "comparison": "==",
            "compare_to": 0
        }
  },
  "invis":
  {
    "type": "origins:invisibility",
    "render_armor": false,
    "condition": 
        {
            "type": "origins:resource",
            "resource": "jorigins:soultravel_poweractive",
            "comparison": "==",
            "compare_to": 0
        }
  },
  "follow":
  {
    "type": "origins:action_over_time",
    "entity_action": {
        "type": "origins:execute_command",
          "command": "function jorigins:followsoul",
          "permission_level": 4
    },
    "interval": 1,
    "condition": {
        "type": "origins:resource",
            "resource": "jorigins:soultravel_poweractive",
            "comparison": "==",
            "compare_to": 0
    }
  },
  "anticollide":
  {
    "type": "origins:prevent_entity_collision",
    "condition": 
        {
            "type": "origins:resource",
            "resource": "jorigins:soultravel_poweractive",
            "comparison": "==",
            "compare_to": 0
        }
  },
  "invuln":
  {
    "type": "origins:invulnerability",
    "condition": 
        {
            "type": "origins:resource",
            "resource": "jorigins:soultravel_poweractive",
            "comparison": "==",
            "compare_to": 0
        }
  }
}